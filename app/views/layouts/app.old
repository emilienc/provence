<!DOCTYPE html>
<html>
<head>
  <title>Provence</title>
  <%= stylesheet_link_tag    :style, :media => "all" %>
  <%= stylesheet_link_tag    'menu/css/simple_menu.css', :media => "all" %>
  <%= stylesheet_link_tag    'css/prettyPhoto.css', :media => "all" %>
  <%= stylesheet_link_tag    'css/gridNavigation.css', :media => "all" %>
  <script src="js/jquery.min.js" type="text/javascript">
  <script src="js/jquery.mousewheel.js" type="text/javascript">
  <script src="js/jquery.gridnav.js" type="text/javascript">
  <script src="js/easing/jquery.easing.1.3.js" type="text/javascript">
  
  <%= csrf_meta_tags %>
  
  <script type="text/javascript">
			$(function() {
				$('#tj_container').gridnav({
					type	: {
						mode		: 'seqfade', 	// use def | fade | seqfade | updown | sequpdown | showhide | disperse | rows
						speed		: 500,			// for fade, seqfade, updown, sequpdown, showhide, disperse, rows
						easing		: '',			// for fade, seqfade, updown, sequpdown, showhide, disperse, rows	
						factor		: 100,			// for seqfade, sequpdown, rows
						reverse		: ''			// for sequpdown
					}
				});
			});
  </script>
</head>
<body>
  
  <%= render 'layouts/header'%>
  
  <div id="container">

  <%= yield %>
  
  </div>
  
  <%= render 'layouts/footer'%>

 
   
</body>
</html>
